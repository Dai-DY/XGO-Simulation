<mujoco model="sdgo">
  <compiler angle="radian" meshdir="../meshes/"/>
  <option timestep="0.002"/>

  <default>
    <default class="robot">
      <default class="motor">
        <joint damping="0.05" armature="0.01" frictionloss="0.05"/>
        <motor gear="1"/>
      </default>
      <default class="visual">
        <geom material="visualgeom" contype="0" conaffinity="0" group="2" />
      </default>
      <default class="collision">
        <geom material="collision_material" condim="3" contype="1" conaffinity="1" group="1" solref="0.005 1" friction="1 0.01 0.01" />
        <equality solimp="0.99 0.999 1e-05" solref="0.005 1" />
      </default>
    </default>
  </default>

  <asset>
    <material name="visualgeom" rgba="0.89 0.89 0.91 1" />
    <material name="collision_material" rgba="0.0 0.4 0.8 0.2" />
    <texture type="2d" name="checker" builtin="checker" rgb1="0.2 0.3 0.4" rgb2="0.9 0.9 0.9" width="500" height="500"/>
    <material name="checker_mat" texture="checker" specular="1" shininess="1" texrepeat="15 15"/>

    <!-- Mesh Assets from URDF -->
    <mesh name="base" file="base.STL" />
    <mesh name="body" file="body.STL" />
    
    <mesh name="fr_hip" file="fr_hip.STL" />
    <mesh name="fr_thigh" file="fr_thigh.STL" />
    <mesh name="fr_calf" file="fr_calf.STL" />
    <mesh name="fr_foot" file="fr_foot.STL" />

    <mesh name="fl_hip" file="fl_hip.STL" />
    <mesh name="fl_thigh" file="fl_thigh.STL" />
    <mesh name="fl_calf" file="fl_calf.STL" />
    <mesh name="fl_foot" file="fl_foot.STL" />

    <mesh name="br_hip" file="br_hip.STL" />
    <mesh name="br_thigh" file="br_thigh.STL" />
    <mesh name="br_calf" file="br_calf.STL" />
    <mesh name="br_foot" file="br_foot.STL" />

    <mesh name="bl_hip" file="bl_hip.STL" />
    <mesh name="bl_thigh" file="bl_thigh.STL" />
    <mesh name="bl_calf" file="bl_calf.STL" />
    <mesh name="bl_foot" file="bl_foot.STL" />
  </asset>

  <worldbody>
    <geom name="floor" type="plane" size="10 10 0.1" pos="0 0 0" material="checker_mat"/>
    <light directional="true" pos="-0.5 0.5 3" dir="0 0 -1"/>

    <!-- Base / Body (Combined because joint is fixed) -->
    <body name="base" pos="0 0 0.25" childclass="robot">
      <freejoint name="floating_base" />
      <!-- Combined mass of base (0.0027) + body (0.18575) approximately -->
      <inertial pos="-0.0034 -0.00025 -0.012" mass="0.188" fullinertia="7.6e-5 5.2e-4 5.5e-4 1.7e-6 9.3e-6 -3.0e-8" />
      
      <geom name="base_vis" type="mesh" mesh="base" class="visual" />
      <geom name="base_col" type="mesh" mesh="base" class="collision" />
      <geom name="body_vis" type="mesh" mesh="body" class="visual" />
      <geom name="body_col" type="mesh" mesh="body" class="collision" />

      <site name="imu_site" pos="0 0 0" size="0.005" rgba="1 0 0 1"/>

      <!-- FRONT RIGHT LEG -->
      <body name="fr_hip" pos="0.062526 -0.02085 0">
        <inertial pos="0.0023 -0.025 0.0055" mass="0.0106" fullinertia="4.1e-6 1.5e-6 4.3e-6 -2.9e-10 -1.5e-8 8.9e-7" />
        <joint name="fr_hip_joint" axis="1 0 0" range="-0.8 0.8" />
        <geom name="fr_hip_vis" type="mesh" mesh="fr_hip" class="visual" />
        <geom name="fr_hip_col" type="mesh" mesh="fr_hip" class="collision" />
        
        <body name="fr_thigh" pos="0.00031911 -0.03686 0">
          <inertial pos="-0.00005 0.0027 -0.028" mass="0.045" fullinertia="1.7e-5 1.6e-5 4.2e-6 3.2e-9 -1.5e-8 3.0e-6" />
          <joint name="fr_thigh_joint" axis="0 1 0" range="-1 2" />
          <geom name="fr_thigh_vis" type="mesh" mesh="fr_thigh" class="visual" />
          <geom name="fr_thigh_col" type="mesh" mesh="fr_thigh" class="collision" />
          
          <body name="fr_calf" pos="-1.45e-05 0 -0.059049">
            <inertial pos="0.0005 -0.0014 -0.035" mass="0.0128" fullinertia="7.4e-6 7.6e-6 5.9e-7 3.9e-9 2.9e-7 5.6e-7" />
            <joint name="fr_calf_joint" axis="0 1 0" range="-2.69 -0.5" />
            <geom name="fr_calf_vis" type="mesh" mesh="fr_calf" class="visual" />
            <geom name="fr_calf_col" type="mesh" mesh="fr_calf" class="collision" />
            
            <body name="fr_foot" pos="0 0 -0.065878">
              <inertial pos="-0.002 -0.00001 -0.002" mass="0.0016" fullinertia="7.5e-8 3.8e-8 7.7e-8 3.2e-10 1.2e-8 1.1e-10" />
              <geom name="fr_foot_vis" type="mesh" mesh="fr_foot" class="visual" />
              <geom name="fr_foot_col" type="mesh" mesh="fr_foot" class="collision" />
            </body>
          </body>
        </body>
      </body>

      <!-- FRONT LEFT LEG -->
      <body name="fl_hip" pos="0.062526 0.020224 0">
        <inertial pos="0.0023 0.0258 0.0055" mass="0.0106" fullinertia="4.1e-6 1.5e-6 4.3e-6 2.9e-10 -1.5e-8 -8.9e-7" />
        <joint name="fl_hip_joint" axis="1 0 0" range="-0.8 0.8" />
        <geom name="fl_hip_vis" type="mesh" mesh="fl_hip" class="visual" />
        <geom name="fl_hip_col" type="mesh" mesh="fl_hip" class="collision" />

        <body name="fl_thigh" pos="0.00031911 0.037482 0">
          <inertial pos="-0.00005 -0.0027 -0.028" mass="0.045" fullinertia="1.7e-5 1.6e-5 4.2e-6 -2.6e-9 -1.4e-8 -2.9e-6" />
          <joint name="fl_thigh_joint" axis="0 1 0" range="-1 2" />
          <geom name="fl_thigh_vis" type="mesh" mesh="fl_thigh" class="visual" />
          <geom name="fl_thigh_col" type="mesh" mesh="fl_thigh" class="collision" />

          <body name="fl_calf" pos="-1.45e-05 0 -0.059049">
            <inertial pos="0.0005 0.0014 -0.035" mass="0.0128" fullinertia="7.4e-6 7.6e-6 5.9e-7 -3.9e-9 2.9e-7 -5.6e-7" />
            <joint name="fl_calf_joint" axis="0 1 0" range="-2.69 -0.5" />
            <geom name="fl_calf_vis" type="mesh" mesh="fl_calf" class="visual" />
            <geom name="fl_calf_col" type="mesh" mesh="fl_calf" class="collision" />

            <body name="fl_foot" pos="-0.00030458 0 -0.065887">
              <inertial pos="-0.0021 0.00001 -0.0022" mass="0.0016" fullinertia="7.5e-8 3.8e-8 7.7e-8 -3.2e-10 1.2e-8 -1.1e-10" />
              <geom name="fl_foot_vis" type="mesh" mesh="fl_foot" class="visual" />
              <geom name="fl_foot_col" type="mesh" mesh="fl_foot" class="collision" />
            </body>
          </body>
        </body>
      </body>

      <!-- BACK RIGHT LEG -->
      <body name="br_hip" pos="-0.05616 -0.02085 0">
        <inertial pos="-0.0096 -0.0258 0.0055" mass="0.0106" fullinertia="4.1e-6 1.5e-6 4.3e-6 -2.9e-10 -1.5e-8 8.9e-7" />
        <joint name="br_hip_joint" axis="1 0 0" range="-0.8 0.8" />
        <geom name="br_hip_vis" type="mesh" mesh="br_hip" class="visual" />
        <geom name="br_hip_col" type="mesh" mesh="br_hip" class="collision" />

        <body name="br_thigh" pos="-0.011685 -0.03686 0">
          <inertial pos="-0.00005 0.002 0.028" mass="0.045" fullinertia="1.7e-5 1.6e-5 4.2e-6 3.2e-9 -1.5e-8 3.0e-6" />
          <joint name="br_thigh_joint" axis="0 1 0" range="-1 2" />
          <geom name="br_thigh_vis" type="mesh" mesh="br_thigh" class="visual" />
          <geom name="br_thigh_col" type="mesh" mesh="br_thigh" class="collision" />

          <body name="br_calf" pos="0 0 -0.059063">
            <inertial pos="0.0007 -0.0021 -0.035" mass="0.0128" fullinertia="7.4e-6 7.6e-6 5.9e-7 1.1e-9 3.3e-7 5.6e-7" />
            <joint name="br_calf_joint" axis="0 1 0" range="-2.69 -0.5" />
            <geom name="br_calf_vis" type="mesh" mesh="br_calf" class="visual" />
            <geom name="br_calf_col" type="mesh" mesh="br_calf" class="collision" />

            <body name="br_foot" pos="0 0 -0.065864">
              <inertial pos="-0.0021 -0.0006 -0.0022" mass="0.0016" fullinertia="7.5e-8 3.8e-8 7.7e-8 3.2e-10 1.2e-8 1.1e-10" />
              <geom name="br_foot_vis" type="mesh" mesh="br_foot" class="visual" />
              <geom name="br_foot_col" type="mesh" mesh="br_foot" class="collision" />
            </body>
          </body>
        </body>
      </body>

      <!-- BACK LEFT LEG -->
      <body name="bl_hip" pos="-0.067842 0.02085 0">
         <inertial pos="0.002 0.0258 0.0055" mass="0.0106" fullinertia="4.1e-6 1.5e-6 4.3e-6 2.9e-10 -1.5e-8 -8.9e-7" />
        <joint name="bl_hip_joint" axis="1 0 0" range="-0.8 0.8" />
        <geom name="bl_hip_vis" type="mesh" mesh="bl_hip" class="visual" />
        <geom name="bl_hip_col" type="mesh" mesh="bl_hip" class="collision" />

        <body name="bl_thigh" pos="0 0.036855 0">
           <inertial pos="-0.00006 -0.002 0.028" mass="0.045" fullinertia="1.7e-5 1.6e-5 4.2e-6 -2.6e-9 -1.4e-8 -2.9e-6" />
          <joint name="bl_thigh_joint" axis="0 1 0" range="-1 2" />
          <geom name="bl_thigh_vis" type="mesh" mesh="bl_thigh" class="visual" />
          <geom name="bl_thigh_col" type="mesh" mesh="bl_thigh" class="collision" />

          <body name="bl_calf" pos="0 0 -0.059063">
             <inertial pos="0.0007 0.002 -0.035" mass="0.0128" fullinertia="7.4e-6 7.6e-6 5.9e-7 -1.1e-9 3.3e-7 -5.6e-7" />
            <joint name="bl_calf_joint" axis="0 1 0" range="-2.69 -0.5" />
            <geom name="bl_calf_vis" type="mesh" mesh="bl_calf" class="visual" />
            <geom name="bl_calf_col" type="mesh" mesh="bl_calf" class="collision" />

            <body name="bl_foot" pos="0 0 -0.065864">
               <inertial pos="-0.0021 0.0006 -0.0022" mass="0.0016" fullinertia="7.5e-8 3.8e-8 7.7e-8 -3.2e-10 1.2e-8 -1.1e-10" />
              <geom name="bl_foot_vis" type="mesh" mesh="bl_foot" class="visual" />
              <geom name="bl_foot_col" type="mesh" mesh="bl_foot" class="collision" />
            </body>
          </body>
        </body>
      </body>
      
    </body>
  </worldbody>

  <actuator>
    <motor name="fr_hip_joint" joint="fr_hip_joint" class="motor" ctrlrange="-0.45 0.45"/>
    <motor name="fr_thigh_joint" joint="fr_thigh_joint" class="motor" ctrlrange="-0.45 0.45"/>
    <motor name="fr_calf_joint" joint="fr_calf_joint" class="motor" ctrlrange="-0.45 0.45"/>
    <motor name="fl_hip_joint" joint="fl_hip_joint" class="motor" ctrlrange="-0.45 0.45"/>
    <motor name="fl_thigh_joint" joint="fl_thigh_joint" class="motor" ctrlrange="-0.45 0.45"/>
    <motor name="fl_calf_joint" joint="fl_calf_joint" class="motor" ctrlrange="-0.45 0.45"/>
    <motor name="br_hip_joint" joint="br_hip_joint" class="motor" ctrlrange="-0.45 0.45"/>
    <motor name="br_thigh_joint" joint="br_thigh_joint" class="motor" ctrlrange="-0.45 0.45"/>
    <motor name="br_calf_joint" joint="br_calf_joint" class="motor" ctrlrange="-0.45 0.45"/>
    <motor name="bl_hip_joint" joint="bl_hip_joint" class="motor" ctrlrange="-0.45 0.45"/>
    <motor name="bl_thigh_joint" joint="bl_thigh_joint" class="motor" ctrlrange="-0.45 0.45"/>
    <motor name="bl_calf_joint" joint="bl_calf_joint" class="motor" ctrlrange="-0.45 0.45"/>
  </actuator>

  <contact>
    <exclude body1="base" body2="fr_hip" />
    <exclude body1="fr_hip" body2="fr_thigh" />
    <exclude body1="fr_thigh" body2="fr_calf" />
    <exclude body1="fr_calf" body2="fr_foot" />
    
    <exclude body1="base" body2="fl_hip" />
    <exclude body1="fl_hip" body2="fl_thigh" />
    <exclude body1="fl_thigh" body2="fl_calf" />
    <exclude body1="fl_calf" body2="fl_foot" />
    
    <exclude body1="base" body2="br_hip" />
    <exclude body1="br_hip" body2="br_thigh" />
    <exclude body1="br_thigh" body2="br_calf" />
    <exclude body1="br_calf" body2="br_foot" />

    <exclude body1="base" body2="bl_hip" />
    <exclude body1="bl_hip" body2="bl_thigh" />
    <exclude body1="bl_thigh" body2="bl_calf" />
    <exclude body1="bl_calf" body2="bl_foot" />
  </contact>

  <sensor>
      <velocimeter name="base_vel" site="imu_site" />
      <accelerometer site="imu_site"/>
      <gyro site="imu_site"/>
  </sensor>

</mujoco>