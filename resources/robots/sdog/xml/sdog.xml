          <joint name="fl_thigh_joint" pos="0 0 0" axis="0 1 0" range="-1 2" actuatorfrcrange="-0.45 0.45"/>
          <geom type="mesh" rgba="0.8902 0.8902 0.91373 1" mesh="fl_thigh"/>
          <body name="fl_calf" pos="-1.4535e-05 0 -0.059049">
            <inertial pos="0.000214512 0.00128085 -0.0394066" quat="0.650178 -0.0312294 0.0218028 0.758827" mass="0.0144904" diaginertia="9.26265e-06 9.11247e-06 6.36453e-07"/>
            <joint name="fl_calf_joint" pos="0 0 0" axis="0 1 0" range="-2.69 -0.5" actuatorfrcrange="-0.45 0.45"/>
            <geom type="mesh" rgba="0.890196 0.890196 0.913725 1" mesh="fl_calf"/>
            <geom pos="-0.00030458 0 -0.065887" quat="1 0 0 0" type="mesh" rgba="0.890196 0.890196 0.913725 1" mesh="fl_foot"/>
