          <joint name="fr_thigh_joint" pos="0 0 0" axis="0 1 0" range="-1 2" actuatorfrcrange="-0.45 0.45"/>
          <geom type="mesh" rgba="0.8902 0.8902 0.91373 1" mesh="fr_thigh"/>
          <body name="fr_calf" pos="-1.4535e-05 0 -0.059049">
