          <joint name="br_thigh_joint" pos="0 0 0" axis="0 1 0" range="-1 2" actuatorfrcrange="-0.45 0.45"/>
          <geom type="mesh" rgba="0.8902 0.8902 0.91373 1" mesh="br_thigh"/>
          <body name="br_calf" pos="0 0 -0.059063">
            <inertial pos="0.000394005 -0.00190142 -0.039386" quat="0.758635 0.0202126 -0.0331094 0.65036" mass="0.0144888" diaginertia="9.26298e-06 9.11248e-06 6.36114e-07"/>
