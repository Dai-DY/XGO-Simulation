            <geom type="mesh" rgba="0.8902 0.8902 0.91373 1" mesh="fr_calf"/>
            <geom pos="0 0 -0.065878" quat="1 0 0 0" type="mesh" rgba="0.8902 0.8902 0.91373 1" mesh="fr_foot"/>
